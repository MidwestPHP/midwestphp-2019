<div class="sponsors-page">
    <div class="background">
        <div class="mask1">
            <div class="mask2">
                {{> navigation }}
            </div>
        </div>
    </div>
    <section>
        <div class="row">
            <div class="small-10 small-centered end">
                <h1>Sponsors
                    <small>.</small>
                </h1>
                <h5 class="level">We Are Proud To Partner With</h5>

                <div class="level-container">
                    <a name="platinum"></a>
                    <h5 class="level">Platinum</h5>
                    <hr>
                    <div class="row medium-centered" data-equalizer data-equalize-by-row="true">
                        {{#each data.platinum}}
                        <div class="large-3 medium-6 column" data-equalizer-watch>
                            <div class="dummy"></div>
                            <div class="img-container thumbnail logo-container">
                                <div class="centerer"></div>
                                <a class="exampleModal1" data-description="{{description}}"
                                    data-logo="{{logo.url}}"
                                    data-company="{{company}}" data-website="{{website}}"
                                    {{#if facebook}}
                                    data-social-facebook="{{facebook}}"
                                    {{/if}}
                                    {{#if github}}
                                    data-social-github="{{github}}"
                                    {{/if}}
                                    {{#if googleplus}}
                                    data-social-googleplus="{{googlepls}}"
                                    {{/if}}
                                    {{#if instagram}}
                                    data-social-instagram="{{instagram}}"
                                    {{/if}}
                                    {{#if linkedin}}
                                    data-social-linkedin="{{linkedin}}"
                                    {{/if}}
                                    {{#if twitter}}
                                    data-social-twitter="{{twitter}}"
                                    {{/if}}
                                >
                                <img src="{{logo.url}}" class="thumbnail" alt="{{company}} logo">
                                </a>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
